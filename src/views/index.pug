extends ./layouts/master.pug

block title
  title My Page - Index

block content
  - var pages = { componentshowcase: '/homepage.html', homepage: '/home-page.html', typography: '/typography.html'}
  - var pagescard = { cardServices: '/card-services.html', cardDetail: '/card-detail.html', cardCompare: '/card-compare.html'}

  //- Desktop page
  section.page.home__page
    .container
      h1.page__title # Index page

      ul(style="list-style: disc; padding-left: 4rem; margin: 4rem 0")
        each val, index in pages
          li
            a(href=val)= index

      h1.page__title # Card
      ul(style="list-style: disc; padding-left: 4rem; margin: 4rem 0")
        each val, index in pagescard
          li
            a(href=val)= index