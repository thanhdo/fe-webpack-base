extends ./layouts/master.pug

block title
  title My Page - Index

block content
  - var pages = {typography: '/typography.html', componentshowcase: '/showcase.html'}
  - var pages2 = {homepage: '/home-page.html'}

  //- Desktop page
  section.page.home__page
    .container
      br
      br
      .row
        .col-lg-4
          h2.page__title # Index page
          ul(style="list-style: disc; padding-left: 4rem; margin: 4rem 0")
            each val, index in pages
              li
                a(href=val)= index

        .col-lg-4
          h2.page__title # Pages
          ul(style="list-style: disc; padding-left: 4rem; margin: 4rem 0")
            each val, index in pages2
              li
                a(href=val)= index